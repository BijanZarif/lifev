[offlinePartioner]
	parts                 = 12
	readPartitionedMeshes = false
	fluidPartitionedMesh  = fluid_P2_12.h5
	solidPartitionedMesh  = solid_P2_12.h5
	interfacePartitioned  = interface_P2_12.h5
	
[../]	
	
[fluid]

	 preconditionerType       = SIMPLE # PCD or SIMPLE
     subiterateFluidDirichlet = false 
     
    	[./physics]
    		density         = 1.0    # density, settings used in the paper
    		viscosity       = 0.03   # viscosity, settings used in the paper

    	[../time_discretization]
    		initialtime     = 0.0
    		endtime	        = 2.00
    		timestep	    = 0.0001
    		BDF_order       = 2

    	[../space_discretization]
    		mesh_dir        = ./
    		mesh_file       = fluido.mesh
    		mesh_type       = .mesh
    		vel_order       = P2
    		pres_order      = P1
    		
    	[../stabilization]
    	    use             = false
    	    type            = SUPGALE
    	    vel_order       = 1	

    	[../]

[../]

[solid]

    	[./space_discretization]
    		mesh_dir        = ./
    		mesh_file       = solido.mesh
    		mesh_type       = .mesh
    		order           = P2

    	[../time_discretization]
		method              = BDF
		initialtime 		= 0.
		endtime     		= 2.00
		timestep    		= 0.0001
		BDF_order   		= 2

	[../model]
		constitutiveLaw     = isotropic
		young     		    = 3.e+6  # Young's modulus, settings used in the paper
		poisson   		    = 0.3    # Poisson's ratio, settings used in the paper
		bulk				= 1e7
		alpha 			    = 2.5e6
		gamma			    = 1.0
		solidTypeIsotropic 	= linearVenantKirchhoff

	[../physics]
		density   		    = 1.2    # density, settings used in the paper
		material_flag       = 1

    	[../]

[../]

[ale]

	[./space_discretization]
		order			= P2

	[./]

[../]

[interface]
	flag        	  =  100       # default: 1, the mesh flag of the interface
	tolerance         =  0       # how far points are to be considered the same on the interface
	fluid_vertex_flag = 13       # tells wether the in/out ring curves on the interface

[../]

[exporter]
    type       = hdf5
    multimesh  = false
    start      = 0
    save       = 1
    fluid_filename  = result_fluid_test
    structure_filename  = result_structure_test

[../]

[newton]
	abstol          		=  1.e-10
	reltol          		=  1.e-5
	etamax          		=  1.e-6
	maxiter         		=  10
	NonLinearLineSearch 	=  0
	output_Residuals    	=  true
	output_Steps        	=  true
	convectiveImplicit      =  true
	useShapeDerivatives     =  false
	extrapolateInitialGuess =  false
	orderExtrapolation      =  2    # it can be 1, 2 or 3
	
[../]
