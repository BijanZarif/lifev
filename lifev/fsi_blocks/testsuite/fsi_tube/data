[offlinePartioner]
	parts                 = 12
	readPartitionedMeshes = false
	fluidPartitionedMesh  = fluid_P2_12.h5
	solidPartitionedMesh  = solid_P2_12.h5
	interfacePartitioned  = interface_P2_12.h5
	
[../]	
	
[fluid]

	 preconditionerType       = SIMPLE # PCD or SIMPLE
     subiterateFluidDirichlet = false 
     
    	[./physics]
    		density         = 1.0    # density
    		viscosity       = 0.035  # viscosity

    	[../time_discretization]
    		initialtime     = 0.0
    		endtime	        = 2.00
    		timestep	    = 0.001
    		BDF_order       = 2

    	[../space_discretization]
    		mesh_dir        = ./
    		mesh_file       = tube20.mesh
    		mesh_type       = .mesh
    		vel_order       = P1
    		pres_order      = P1

        [../stabilization]
            use             = true
            type            = SUPGALE
            vel_order       = 1

    	[../]

[../]

[solid]

    	[./space_discretization]
    		mesh_dir        = ./
    		mesh_file       = vessel20.mesh
    		mesh_type       = .mesh
    		order           = P1

    	[../time_discretization]
		method              = BDF
		initialtime 		= 0.
		endtime     		= 2.00
		timestep    		= 0.001
		BDF_order   		= 2

	[../model]
		constitutiveLaw     = isotropic
		young     		= 4.e+6
		poisson   		= 0.3
		bulk				= 1e7
		alpha 			= 2.5e6
		gamma			= 1.0
		solidTypeIsotropic 	= linearVenantKirchhoff

	[../physics]
		density   		= 1.2
		material_flag       = 1

    	[../]

[../]

[ale]

	[./space_discretization]
		order			= P1

	[./]

[../]

[interface]
	flag        	   =  1       # default: 1, the mesh flag of the interface
	tolerance         =  0       # how far points are to be considered the same on the interface
	fluid_vertex_flag = 20       # tells wether the in/out ring curves on the interface

[../]

[exporter]
    type       = hdf5
    multimesh  = false
    start      = 0
    save       = 1
    fluid_filename  = result_fluid
    structure_filename  = result_structure

[../]

[newton]
	abstol          		=  1.e-6
	reltol          		=  1.e-6
	etamax          		=  1.e-6
	maxiter         		=  10
	NonLinearLineSearch 	=  0
	output_Residuals    	=  true
	output_Steps        	=  true
	convectiveImplicit      	=  true
	useShapeDerivatives     	=  true
	extrapolateInitialGuess  =  true
	orderExtrapolation       =  2    # it can be 1, 2 or 3
	
[../]
