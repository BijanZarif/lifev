* #offline partitioner details
[offline_partitioner]
useOfflinePartitionedMesh = false                  # true for loading an already partitioned mesh, false to partition the mesh online
num_parts = 6                                     # must be the same of the number of parts when we partioned offline the mesh
hdf5_file_name = cylinder_offlinePartitioned.h5   # file generated running test partion_io (main_write) - REQUIRED - 
io_class = new                                    # possibilities: old or new, use new

[exporter]
type       = hdf5 # hdf5 (if library compiled with hdf5 support) or ensight
multimesh  = false
start      = 0
save       = 1

[NavierStokes]
initialization         = interpolation #projection   # initialization (L2 projection) or interpolation
export_norms           = true
export_exact_solutions = true
test                   = accuracy
accuracy_tolerance     = 0.09
mesh_source            = regular_mesh

[fluid]

    export_coefficients = true  
    conservativeFormulation = false
    
    [./physics]
    density         = 1000       # density
    viscosity       = 1          # viscosity

    [../time_discretization]
    initialtime     = 0.0
    endtime         = 14.945
    timestep        = 0.0025

    BDF_order       = 1 

    [../space_discretization]
    mesh_dir        = ./
    mesh_file       = cylinder_new_longer.mesh
    mesh_type       = .mesh

    verbose         = 0
    linearized      = 0
    diagonalize     = 1 # weight, 0=off
    div_beta_u_v    = 0 # 1=on, 0=off
    FE_number       = 1
    vel_order       = P1
    pres_order      = P1
    stiff_strain    = true
    
	[../stabilization]
	use             = true
	method          = VMSLES	  # other possible choices: IP, SUPG, VMSLES, SUPGVMS

    [../miscellaneous]
    verbose         = 1
    steady          = 0

    [../prec]
    prectype                = ML # Ifpack or ML
    displayList             = true
    
        [./ML]

        default_parameter_list  = NSSA  # for ML precond, SA, DD, DD-ML, maxwell, NSSA, DD-ML-LU, DD-LU
        inc_or_dec              = increasing
	    max_levels              = 3
        MLOutput                = 10
        pde_equations           = 4
        cycle_applications      = 3

            [./smoother]
            # smoother type
            # Aztec, IFPACK, Jacobi, ML symmetric Gauss-Seidel, symmetric Gauss-Seidel,
            # ML Gauss-Seidel, Gauss-Seidel, Chebyshev, MLS, Hiptmair, Amesos-KLU,
            # Amesos-Superlu, Amesos-UMFPACK, Amesos-Superludist, Amesos-MUMPS,
            # user-defined, SuperLU, IFPACK-Chebyshev, self, do-nothing,
            # IC, ICT, ILU, ILUT
            type            = Gauss-Seidel
            pre_or_post     = both
            sweeps          = 3

            [../coarse]
            type                    = Amesos-KLU
            sweeps                  = 1
            pre_or_post             = both
 
	    [../aggregation]
            threshold               = 0.01
            type                    = Uncoupled-MIS      
      
            [../energy_minimization]
            enable                  = true

	    [../repartition]
            enable                  = true
            partitioner             = ParMETIS    


        # ifpack
        [../ifpack]
        overlap     = 2

            [./fact]
            ilut_level-of-fill            = 1
            drop_tolerance                = 1.e-5
            relax_value                   = 0

            [../amesos]
            solvertype =  Amesos_Umfpack # Amesos_KLU or Amesos_Umfpack

            [../partitioner]
            overlap = 2

            [../schwarz]
            reordering_type = none #metis, rcm, none
            filter_singletons = true

            [../]
        [../]

	[../solver]
    solver          = gmres
    scaling         = none
    output          = all # none
    conv            = rhs
    max_iter        = 1800
    reuse           = true
    max_iter_reuse  = 80
    kspace          = 100
    tol             = 1e-8    # AztecOO tolerance
    
    [../ipstab]
