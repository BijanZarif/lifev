* #offline partitioner details
[offline_partitioner]
useOfflinePartitionedMesh = true                  					# true for loading an already partitioned mesh, false to partition the mesh online
num_parts = 18                                     					# must be the same of the number of parts when we partioned offline the mesh
hdf5_file_name = cylinder_coarse_18_MeshPartitionTool_ParMETIS.h5                       # file generated running test partion_io (main_write) - REQUIRED - 
io_class = new                                    					# possibilities: old or new, use new

[exporter]
type       = hdf5 # hdf5 (if library compiled with hdf5 support) or ensight
multimesh  = false
start      = 0
save       = 1
filename   = result_fluid

[importer]
restart  = false
initSol  = 00007        # label of the solution from which we want to start (5 digits by convention)
type     = hdf5         # format of the imported solution
filename = resultBDF2_parte1

[fluid]

    options_preconditioner = solversOptionsFast
    preconditionerType  = aSIMPLEOperator
    save_every          = 1
    export_coefficients = true  
    conservativeFormulation = false
    
    [./physics]
    density         = 1.0    # density
    viscosity       = 0.035  # viscosity

    [../time_discretization]
    initialtime     = 0.0
    endtime         = 0.2
    timestep        = 0.001

    BDF_order       = 2

    [../space_discretization]
    mesh_dir        = ./
    mesh_file       = tube20.mesh
    mesh_type       = .mesh

    verbose         = 0
    linearized      = 0
    diagonalize     = 1 # weight, 0=off
    div_beta_u_v    = 0 # 1=on, 0=off
    FE_number       = 1
    vel_order       = P2
    pres_order      = P1
    stiff_strain    = true
    
	[../stabilization]
	use             = false
	method          = SUPG	  # other possible choices: IP, SUPG, VMSLES, SUPGVMS

    [../miscellaneous]
    verbose         = 1
    steady          = 0
