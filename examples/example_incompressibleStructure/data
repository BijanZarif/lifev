[../solver]
solver          = gmres
scaling         = none
output          = all # none
conv            = r0
max_iter        = 100
kspace          = 100
tol             = 1.e-5
	

[../prec]
prectype           = none #Ifpack
reuse              = true
displayList        = true

[./ML]
default_parameters = DD    # for ML precond, SA, DD, DD-ML, maxwell, NSSA, DD-ML-LU, DD-LU

# Parameters for the Ifpack preconditioner

[../ifpack]
prectype               = Amesos    # Amesos (does a local LU factorization), ILUT (not ILU)
overlap                = 5

[./relaxation]
type                   = Jacobi
damping_factor         = 1.
