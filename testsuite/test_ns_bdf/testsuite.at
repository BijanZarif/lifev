AT_SETUP([test_ns_bdf])
AT_KEYWORDS([ns bdf])
AT_DATA([data],[[# -*- getpot -*- (GetPot mode activation for emacs)
#-------------------------------------------------
#        Data file for NavierStokes ACT PC
#-------------------------------------------------



[fluid]

[./physics]
density         = 1.0               # density
viscosity       = 0.035             # viscosity
endtime         = 0.0002            # endtime --> two time step for testing purpose


[../discretization]
mesh_dir = ./
mesh_file = cyltetra.mesh       # mesh file
timestep  = 0.0001              # timestep
order_bdf = 3                   # order of the time integration scheme

[../miscellaneous]
verbose         = 3

[../aztec_i]
output          = warnings #all_res
max_iter        = 1550
poly_ord        = 5
kspace          = 40
precond         = dom_decomp
tol             = 1.00e-6
drop            = 1.00e-4
ilut_fill       = 1.5

[../aztec_ii]
output          = warnings #all_res
conv            = rhs
max_iter        = 1550
poly_ord        = 5
kspace          = 40
tol             = 1.00e-6
drop            = 1.00e-4
ilut_fill       = 1.5


[../aztec_s]
output          = warnings
conv            = rhs
poly_ord        = 5
kspace          = 40
tol             = 1.00e-6
drop            = 1.00e-4
]])
AT_CHECK([ln -sf $top_builddir/testsuite/data/mesh/inria//cyltetra.mesh &&  $top_builddir/testsuite/test_ns_bdf/test_ns_bdf],[0],[ignore],[ignore])
AT_CLEANUP
