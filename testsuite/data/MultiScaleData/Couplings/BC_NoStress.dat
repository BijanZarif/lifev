###################################################################################################
### COUPLING: BoundaryCondition - NoStress ########################################################
###################################################################################################

[MultiScale]
couplingName	= BoundaryCondition_NoStress

[boundary_conditions]
list = 'NoStress' # NoStress

	[./NoStress]
	type			= Natural
	mode			= Normal
	function		= 0
	
    # Additional parameters for 1D BC
    quantity        = P
    line            = first
	
	[../]