###################################################################################################
### COUPLING: BoundaryCondition - Cylinder Poiseuille Flux ########################################
###################################################################################################

[Multiscale]
couplingName	= BoundaryCondition_PoiseuilleFlux

[boundary_conditions]
list = 'PoiseuilleFlux'  # Poiseuille Flux
	
	[./PoiseuilleFlux]
	type				= Flux
	mode				= Full
	component			= 3
	function   			= '-(pi*1)/(8*0.035)*(0.5^4)'                       #TEMPORARY FIX
	#OPERfunction		= '-(pi*f_density)/(8*f_viscosity)*((f_area/pi)^2)'
	
    # Additional parameters for 1D BC
    quantity        = Q
    line            = first
    
	[../]
