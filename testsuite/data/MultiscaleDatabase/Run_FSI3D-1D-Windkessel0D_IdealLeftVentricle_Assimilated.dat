###################################################################################################
### MULTISCALE: FSI3D Model for the left ventricle ################################################
###################################################################################################

[Problem] #########################################################################################
ProblemType = Multiscale
ProblemFile = ./MultiscaleDatabase/Models/Multiscale/FSI3D-1D-Windkessel0D_IdealLeftVentricleWithArterialTree_Assimilated_S

[Solver] ##########################################################################################
###################################################################################################

    [./Restart]
    Restart                    = false
    RestartFromStepNumber      = 0

    [../Output]
    ProblemPrefix              = Multiscale # Problem prefix
    SaveEach                   = 1          # time steps

    [../time_discretization]
    initialtime                = 0.000   # [s]
    endtime                    = 0.800   # [s]
    timestep                   = 0.001   # [s]

    [../]

###################################################################################################

[Physics] #########################################################################################
###################################################################################################
FluidDensity                = 1.04          # density        [g/cm^3]
FluidViscosity              = 0.035         # viscosity      [g/cm/s]
FluidVenousPressure         = 6.6661195E+03 # ven. pressure  [g/cm/s^2 - dyne/cm^2]
SolidExternalPressure       = 1E+05         # ref. pressure  [g/cm/s^2 - dyne/cm^2]
SolidDensity                = 1.2           # density        [g/cm^3]
SolidPoissonCoefficient     = 0.4           # Poisson coeff. []
SolidYoungModulus           = 7.0E6         # Young modulus  [g/cm/s^2 - dyne/cm^2]
###################################################################################################
