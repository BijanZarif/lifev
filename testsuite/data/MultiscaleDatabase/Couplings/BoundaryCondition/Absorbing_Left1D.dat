###################################################################################################
### COUPLING: BoundaryCondition - Absorbing (1D - left) ###########################################
###################################################################################################

[Multiscale]
couplingName    = BoundaryCondition_Absorbing_1DLeft

[boundary_conditions]
list = 'OutFlow_absorbing'
    
    [./OutFlow_absorbing]
    quantity            = W1
    line                = first
    Default             = Absorbing
    
    [../]
