###################################################################################################
### COUPLING: BoundaryCondition - Template ########################################################
###################################################################################################

[Multiscale]
couplingName    = BoundaryCondition_Template

[boundary_conditions]
list = 'TemplateBC'
    
    [./TemplateBC]
    type                = Flux
    mode                = Full
    component           = 3
    function            = '0' 
    
    # Additional parameters for 1D BC
    quantity            = Q
    line                = first
    
    [../]