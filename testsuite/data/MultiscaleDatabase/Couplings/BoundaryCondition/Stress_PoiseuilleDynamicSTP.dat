###################################################################################################
### COUPLING: BoundaryCondition - Pressure Dynamic ################################################
###################################################################################################

[Multiscale]
couplingName    = BoundaryCondition_PressureDynamic

[boundary_conditions]
list = 'PressureDynamic'

    [./PressureDynamic]
    type                = Natural
    mode                = Normal
    functionFile        = physicalFunctions/SmoothTrapezoidalProfile.dat[C]

    # Additional parameters for 1D BC
    quantity            = P
    line                = first
    
    [../]
