[problem]

method     = 2 # 0 = fixedpoint, 1 = steklovpoincare, 2 = exactjacobian	
precond    = 1 # precond for steklovpoincare, 0 = lin fluid, 1 lin structure, 2 = both
defOmega   = 0.01 # usually 0.01 for precond = 0 or 1; -1 for precond = 2
defOmegaS  = 0.005 # matters only with  precond = 2
defOmegaF  = 0.005 # matters only with  precond = 2


[fluid]

[./physics]
endtime   = 0.002             # endtime
density   = 1.0               # density
viscosity = 0.03              # viscosity


[../discretization]
