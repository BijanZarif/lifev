AT_SETUP([test_fsi])
AT_KEYWORDS([ns fluid solid fsi])
AT_DATA([data],[[# -*- getpot -*- (GetPot mode activation for emacs)
#-------------------------------------------------
#      Data file for Fluid Structure Solver 
#-------------------------------------------------

[problem]

method     = 2 # 0 = fixedpoint, 1 = steklovpoincare, 2 = exactjacobian	
precond    = 1 # precond for steklovpoincare, 0 = lin fluid, 1 lin structure, 2 = both
defOmega   = 0.01 # usually 0.01 for precond = 0 or 1; -1 for precond = 2
defOmegaS  = 0.005 # matters only with  precond = 2
defOmegaF  = 0.005 # matters only with  precond = 2


[fluid]

[./physics]
endtime   = 0.002             # endtime
density   = 1.0               # density
viscosity = 0.03              # viscosity


[../discretization]
mesh_dir  = ./ # the directory where the mesh file is
mesh_file = tube_curve20.mesh  # mesh file
timestep  = 0.001              # timestep

[../miscellaneous]
verbose   = 1


[solid]

[./physics]
density   = 1.2               # density
young     = 3.0E6
poisson   = 0.3
endtime   = 0.002

[../discretization]
mesh_dir  = ./   # the directory where the mesh file is
mesh_file = vessel_curve20.mesh  # mesh file
timestep  = 0.001             # timestep

[../miscellaneous]
factor    = 10
verbose   = 1


[../newton]
maxiter = 1
abstol  = 1.e-8
linesearch = 0


[../aztec]
output          = none #all_res
max_iter        = 30
poly_ord        = 5
kspace          = 30
precond         = dom_decomp
drop            = 1.00e-4
ilut_fill       = 2
tol             = 1.e-16


[fluid]

[./aztec_i]
output          = all_res
max_iter        = 1550
poly_ord        = 5
kspace          = 40
precond         = dom_decomp
tol             = 1.00e-6
drop            = 1.00e-4
ilut_fill       = 1.5

[../aztec_ii]
output          = all_res
conv            = rhs
max_iter        = 1550
poly_ord        = 5
kspace          = 40
tol             = 1.00e-6
drop            = 1.00e-4
ilut_fill       = 1.5


[../aztec_s]
output          = none #all_res
conv            = rhs
poly_ord        = 5
kspace          = 40
tol             = 1.00e-6
drop            = 1.00e-4


[jacobian]

[../aztec]
output          = none #all_res
max_iter        = 30
poly_ord        = 5
kspace          = 30
precond         = dom_decomp
drop            = 1.00e-4
ilut_fill       = 2
tol             = 1.e-16
]])
AT_CHECK([ln -sf $top_builddir/testsuite/data/mesh/inria/tube_curve20.mesh &&
          ln -sf $top_builddir/testsuite/data/mesh/inria/vessel_curve20.mesh &&
          $top_builddir/testsuite/test_fsi/test_fsi],[0],[ignore],[ignore])
AT_CLEANUP([out_res_solid iter num_iter out_iter_solid res])

