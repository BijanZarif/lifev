# -*- getpot -*- (GetPot mode activation for emacs)
#-------------------------------------------------
#   Data file for One D Model (Non Linear)
#-------------------------------------------------

[boundcond]
test_case	= 3	# test case for boundary conditions

[parameters]
# flux matrix for the linear test case
flux11	= 17.
flux12	= -60.
flux21	= 5.
flux22	= -18.
#eigenvalues of the flux matrix for the linear test case
celer1	= 2.		# celerity (1rst comp)
celer2	= -3.		# celerity (2nd comp)
#left eigenvectors of the flux matrix for the linear test case
left_eigvec11 = 1.
left_eigvec12 = -3.
left_eigvec21 = -1.
left_eigvec22 = 4.
# source function for the linear test case
source10	= 0.
source20	= 0.
source11	= 8
source12	= -12.
source21	= 1.
source22	= 1.

[time]
timebeg	 = 0.0		# inittime
timeend	 = 10.0		# endtime
timestep = 0.01		# timestep		

[discretization]
mesh_file	= cylhexa.mesh	# mesh file name
mesh_dir	= ../data/mesh/inria 
		# the directory where the mesh file is
x_left	= 0.
x_right	= 10.
nb_elem	= 100	#   number of elements 

[miscellaneous]
post_dir = ./Post                 # the directory for postprocessing files
verbose = 5
post_proc_format = medit          # vtk or medit
show_graceplot = 1                # 0: no grace plot, 1: show grace plots


[aztec]  
solver = cg  #cg , gmres, cgs, tfqmr, bicgstab, slu, symmlq, fixed_pt, lu
precond = dom_decomp
subdomain_solve = icc
output = warnings    #warnings , none , all, all_res, last
tol = 1e-6

