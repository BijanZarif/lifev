# -*- getpot -*- (GetPot mode activation for emacs)
#----------------------------------------------------------------
#      Data file for cylinder test with Chorin-Temam 
#----------------------------------------------------------------

[exporter]
multimesh  = false
start      = 0
save       = 5

[fluid]

[./physics]
density         = 1.0       
viscosity       = 0.005 #0.005 

[../time_discretization]
initialtime		= 0.
endtime			= 2.0
timestep		= 0.035                 # timestep, e.g. 0.05 for implicit stabilization
                                        #            and 0.035 for explicit stabilization
BDF_order_vel	= 1		# 1, 2, 3
BDF_order_press = 0		# 0 (standard Chorin-Temam), 1, 2, 3

[../space_discretization]
mesh_dir        = ./Mesh/
#mesh_file       = cyl3D-2.mesh         # approx. 140,000 tets
mesh_file       = cyl3D.2.2.00.mesh	# approx. 29,000 tets
mesh_type       = .mesh

verbose         = 0                     # verbose during mesh-reading
linearized      = 0
diagonalize     = 0		# weight, 0=off
div_beta_u_v    = 0 		# 1=on, 0=off
vel_order       = P1		# P1, P1Bubble, P2
press_order     = P1		# P1, P2
variationalPn	= 0		# 0: standard (v|grad p) couling, 1: (div v|p) coupling 


[../solver]
solver          = gmres
scaling         = none
output          = all # none
conv            = rhs
max_iter        = 100
kspace          = 100

# see http://trilinos.sandia.gov/packages/docs/r8.0/packages/ifpack/doc/html/ifp_ilu.html
[../prec]
prectype        = Ifpack	# Ifpack or ML 
reuse           = true
overlap         = 2
#droptol         = 1.e-5 
fill            = 4
relax_value     = 1

[../stabilization]
stabtype	= ip_stab_expl	# sd_stab, ip_stab_impl, or ip_stab_expl

[../sdstab]
gammaBeta	= 0.02		# 0.02 
gammaDiv 	= 0.2 		# 0.2

[../ipstab]
gammaBeta	= 0.01 		# eg 0.02 for implicit and 0.01 for explicit
gammaDiv	= 0.00 		# 0.2, not used in explicit

[../miscellaneous]
steady		= 0

[../problem]
Re		= 1.0
nu		= 1.5
D		= 1.0
H		= 20.0
centered	= 1
amplitude	= 1.0
tamplitude	= 2.0
