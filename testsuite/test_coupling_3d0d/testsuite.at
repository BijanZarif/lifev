AT_SETUP([test_coupling_3d0d (non compliant)])
AT_KEYWORDS([coupling d30d noncompliant])

AT_DATA([data],[[# -*- getpot -*- (GetPot mode activation for emacs)
#-------------------------------------------------
#        Data file for NavierStokes ACT PC
#-------------------------------------------------

[problem]

strategy = 2   # coupling strategy : 1 -> mean pressure; 2 -> Flow rate


[fluid]

[./physics]
density         = 1.0               # density
viscosity       = 0.035             # viscosity
inittime        = 0.00              # starttime
endtime         = 0.002             # endtime --> two steps for testing purposes

[../geometry]
length = 5.0                        # length of the cylinder
radius = 0.5	                    # radius of the cylinder

[../discretization]
mesh_dir = ./
mesh_file = cyltetra.mesh      # mesh file
timestep  = 0.001               # timestep


[../miscellaneous]
verbose         = 1

[../aztec_i]
output          = warnings #all_res
max_iter        = 1550
poly_ord        = 5
kspace          = 40
precond         = dom_decomp
tol             = 1.00e-6
drop            = 1.00e-4
ilut_fill       = 1.5

[../aztec_ii]
output          = all_res
conv            = rhs
max_iter        = 1550
poly_ord        = 5
kspace          = 40
tol             = 1.00e-6
drop            = 1.00e-4
ilut_fill       = 1.5


[../aztec_s]
output          = warnings #all_res
conv            = rhs
poly_ord        = 5
max_iter        = 1550
kspace          = 40
tol             = 1.00e-6
drop            = 1.00e-4
]])
AT_CHECK([ln -sf $top_builddir/testsuite/data/mesh/inria/cyltetra.mesh &&  $top_builddir/testsuite/test_coupling_3d0d/test_coupling_NS0d -f data],[0],[ignore],[ignore])
AT_CLEANUP








AT_SETUP([test_coupling_3d0d (compliant)])
AT_KEYWORDS([coupling 3d0d compliant])

AT_DATA([data],[[# -*- getpot -*- (GetPot mode activation for emacs)
#-------------------------------------------------
#      Data file for Fluid Structure Solver 
#-------------------------------------------------


[problem]

strategy = 1  # coupling strategy : 1 -> mean pressure; 2 -> Flow rate

method     = 2 # 0 = fixedpoint, 1 = steklovpoincare, 2 = exactajcobian	
precond    = 1 # precond for steklovpoincare, 0 = lin fluid, 1 lin structure, 2 = both
defOmega   = 0.01 # usually 0.01 for precond = 0 or 1; -1 for precond = 2
defOmegaS  = 0.005 # matters only with  precond = 2
defOmegaF  = 0.005 # matters only with  precond = 2

[fluid]

[./physics]
endtime   = 0.002             # endtime
density   = 1.0               # density
viscosity = 0.03              # viscosity

[../geometry]
length = 5.0                        # length of the cylinder
radius = 0.5	                    # radius of the cylinder
thickness = 0.1                     # thickness of the wall

[../discretization]
mesh_dir  = ./                  # the directory where the mesh file is
mesh_file = tube.mesh           # mesh file
timestep  = 0.001               # timestep

[../miscellaneous]
verbose   = 1


[solid]

[./physics]
density   = 1.2               # density
young     = 3.0E6
poisson   = 0.3
endtime   = 0.002

[../discretization]
mesh_dir  = ./                    # the directory where the mesh file is
mesh_file = vessel.mesh           # mesh file
timestep  = 0.001                 # timestep

[../miscellaneous]
factor    = 10
verbose   = 1


[../newton]
maxiter = 1
abstol  = 1.e-8
linesearch = 0


[../aztec]
output          = none #all_res
max_iter        = 300
poly_ord        = 5
kspace          = 40
precond         = dom_decomp
drop            = 1.00e-4
ilut_fill       = 2
tol             = 1.e-16


[fluid]

[./aztec_i]
output          = all_res
max_iter        = 1550
poly_ord        = 5
kspace          = 40
precond         = dom_decomp
tol             = 1.00e-6
drop            = 1.00e-4
ilut_fill       = 1.5

[../aztec_ii]
output          = all_res
conv            = rhs
max_iter        = 1550
poly_ord        = 5
kspace          = 40
tol             = 1.00e-6
drop            = 1.00e-4
ilut_fill       = 1.5


[../aztec_s]
output          = none
conv            = rhs
poly_ord        = 5
kspace          = 40
tol             = 1.00e-6
drop            = 1.00e-4


]])
AT_CHECK([ln -sf $top_builddir/testsuite/data/mesh/inria/tube.mesh &&
          ln -sf $top_builddir/testsuite/data/mesh/inria/vessel.mesh &&
          $top_builddir/testsuite/test_coupling_3d0d/test_coupling_FSI0d -f data],[0],[ignore],[ignore])
AT_CLEANUP([out_res_solid iter num_iter out_iter_solid res])

