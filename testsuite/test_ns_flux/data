# -*- getpot -*- (GetPot mode activation for emacs)
#----------------------------------------------------------------
#      Data file for cylinder test with Navier-Stokes
#----------------------------------------------------------------

[exporter]
multimesh  = false
start      = 0
save       = 1


[fluid]

[./problem]
# for  cyl3D.2.2.00.mesh
Re              = 1
D               = 1.
H               = 2.
centered        = 1
initial_sol     = stokes          # stokes, none


[../physics]
density         = 1.0          # density
viscosity       = 0.03         # viscosity

[../time_discretization]
initialtime		= 0.
endtime			= 0.0
timestep		= 0.0001
BDF_order		= 1

[../space_discretization]
mesh_dir        = ./Mesh/
mesh_file       = tube40.mesh          # "big file"
#mesh_file       = tube-test.mesh          # "big file"
#mesh_file       = tube-r5.mesh          # "big file"
#mesh_file       = cyl3D-1.mesh          # "big file"
#mesh_file       = cyl3D.2.2.00.mesh
#mesh_file       = cyl2D.mesh             # "reasonable file"
mesh_type       = .mesh

verbose         = 0                     # verbose during mesh-reading
linearized      = 0
diagonalize     = 0 # weight, 0=off
div_beta_u_v    = 0 # 1=on, 0=off
vel_order       = P1Bubble		# P1, P1Bubble, P2
press_order     = P1		# P1, P2

[../solver]
solver          = gmres
scaling         = none
output          = all # none
conv            = rhs
max_iter        = 200
kspace          = 200
tol             = 1.e-10    # AztecOO tolerance


[../prec] #see http://trilinos.sandia.gov/packages/docs/r8.0/packages/ifpack/doc/html/ifp_ilu.html
prectype        = ML # Amesos (does a local LU factorization), ILUT (not ILU)
reuse           = true
displayList	= 0
                [./ML]
                analyze_smoother        = false
                default_parameter_list  = none    	# for ML precond, SA, DD, DD-ML, maxwell, NSSA, DD-ML-LU, DD-LU
		max_levels		= 3
		prec_type 		= MGV		# two-level-additive
		inc_or_dec		= increasing	 # decreasing increasing


			[./smoother]
			type                    = IFPACK
			pre_or_post             = post
			sweeps                  = 1
			damping_factor          = 1.333

			[../coarse]
			#type                    = Gauss-Seidel
			type                    = Amesos-Superlu
			sweeps			= 1
			pre_or_post   		= post
			max_size                = 128


		[../ifpack]
		prectype        = Amesos    # Amesos (does a local LU factorization), ILUT (not ILU)
		droptol         = 1.e-5
		fill            = 4
		relax_value     = 1
		overlap         = 1

			[./amesos]
			solvertype      = Amesos_Umfpack

[../ipstab]
#gammaBeta  = 0.02
#gammaDiv   = 0.02
#gammaPress = 0.02
#gammaBeta  = 0.125
#gammaDiv   = 0.125
#gammaPress = 0.03125
reuse      = true

