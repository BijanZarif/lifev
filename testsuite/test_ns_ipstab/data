# -*- getpot -*- (GetPot mode activation for emacs)
#-------------------------------------------------
#      Data file for Navier-Stokes IP Solver
#-------------------------------------------------

[fluid]

[./physics]
endtime   = 5.0          # endtime
density   = 1.0          # density
viscosity = 0.0001       # viscosity


[../discretization]
mesh_dir   = ../data/mesh/inria/           # the directory where the mesh file is
mesh_file  = hexa4x4.mesh # mesh file
mesh_faces = all          # update all faces elements
timestep   = 1.0          # timestep

[../miscellaneous]
verbose   = 1
steady    = 0

[../aztec]
solver    = gmres
scaling   = none
output    = all_res
conv      = r0
max_iter  = 1550
kspace    = 200
precond   = dom_decomp
subdomain_solve = ilu
ilut_fill = 6   
drop      = 1.e-5
tol       = 1.00e-10

[../petsc]
ksp_type                  = gmres
ksp_gmres_restart         = 200
ksp_rtol                  = 1.e-10
ksp_max_it                = 1550
pc_type                   = ilu
pc_ilu_use_drop_tolerance = 1.e-1,0.1,100 # activates SPARSKIT routine
