AT_SETUP([test_ns_ipstab])
AT_KEYWORDS([ns ipstab])
AT_DATA([data],[[# -*- getpot -*- (GetPot mode activation for emacs)
#-------------------------------------------------
#      Data file for Navier-Stokes IP Solver
#-------------------------------------------------

[fluid]

[./physics]
endtime   = 5.0          # endtime
density   = 1.0          # density
viscosity = 0.0001       # viscosity


[../discretization]
mesh_dir   = ../data/mesh/inria/           # the directory where the mesh file is
mesh_file  = hexa4x4.mesh # mesh file
mesh_faces = all          # update all faces elements
timestep   = 1.0          # timestep

[../miscellaneous]
verbose   = 1

[../aztec]
output    = all_res
conv      = r0
max_iter  = 1550
kspace    = 200
precond   = dom_decomp
ilut_fill = 6   
drop      = 1.e-5
tol       = 1.00e-10
]])
AT_CHECK([$top_builddir/testsuite/test_ns_ipstab/test_ns_ipstab],[0],[ignore],[ignore])
AT_CLEANUP
