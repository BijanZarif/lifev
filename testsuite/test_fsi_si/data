# -*- getpot -*- (GetPot mode activation for emacs)
#-------------------------------------------------
#      Data file for Fluid Structure Solver 
#-------------------------------------------------


FSIalgo = 3   # 1 Exact Newton, 2 Quasi-Newton (linearized ns), 3 Quasi-Newton (reduced fluid)

[fluid]

[./physics]
endtime   = 0.02             # endtime
density   = 1.0               # density
viscosity = 0.03              # viscosity


[../discretization]
mesh_dir  = ../data/mesh/inria/         # the directory where the mesh file is
mesh_file =  tube20.mesh           # mesh file
#mesh_faces = all           $ MANDATORY for ip stabilization
timestep  = 0.001 
stabilization = sd #  sd (SDFEM), ip (Interior Penalty)    

[../miscellaneous]
verbose         = 1
factor          = 10

[../aztec]
solver          = gmres
output          = all_res
conv            = r0
max_iter        = 300
kspace          = 100
precond         = dom_decomp #none,jacobi,sym_gs,neumann,ls,smoother,dom_decomp
subdomain_solve = ilut
ilut_fill       = 8
drop            = 1.e-7
tol             = 1.00e-10
keep_info       = 1

[../reduced_aztec]
output          = none
conv            = r0
max_iter        = 300
kspace          = 100
precond         = dom_decomp #none,jacobi,sym_gs,neumann,ls,smoother,dom_decomp
subdomain_solve = ilut
ilut_fill       = 6
drop            = 1.e-5
tol             = 1.00e-10
keep_info       = 1

[../sdstab]
gammaBeta  = 0.1
gammaDiv   = 0.1

[../ipstab]
gammaBeta  = 0.00125
gammaDiv   = 0.00125
gammaPress = 0.0003125

[mesh_motion]

[./aztec]
output          = none
max_iter        = 300
poly_ord        = 5
kspace          = 100
precond         = dom_decomp
drop            = 1.00e-4
ilut_fill       = 4
tol             = 1.e-10
keep_info       = 1





[solid]

[./physics]
density   = 1.2               # density
young     = 3.0E6
poisson   = 0.3
endtime   = 0.02

[../discretization]
mesh_dir  = ../data/mesh/inria/   # the directory where the mesh file is
mesh_file = vessel20.mesh  # mesh file
timestep  = 0.001             # timestep

[../miscellaneous]
factor    = 10
verbose   = 1


[../newton]
maxiter = 1
abstol  = 1.e-8
linesearch = 0


[../aztec]
output          = none #all_res
max_iter        = 300
poly_ord        = 5
kspace          = 40
precond         = dom_decomp
drop            = 1.00e-4
ilut_fill       = 2
tol             = 1.e-16



