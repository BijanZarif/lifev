AT_SETUP([test_mass_coupling])
AT_KEYWORDS([mass_coupling])
AT_DATA([data-lumen],[[# -*- getpot -*- (GetPot mode activation for emacs)
#-------------------------------------------------
#   Data file for the masstransport 
#-------------------------------------------------

[masstransport]

[./physics]
diffusivity     = 2.87e-7           # diffusivity
react           = 0.0               # reaction coefficient
stationary      = 1                 # stationary calculation

# Till now we compute fluid and mastransport on the same mesh
[../discretization]
mesh_dir  = ../data/mesh/inria/    # the directory where the mesh file is
mesh_file = piece-of-tube.mesh     # mesh file

[../aztec_o]
output          = warnings #all_res
max_iter        = 1550
poly_ord        = 5
kspace          = 40
precond         = dom_decomp
tol             = 1.00e-6
drop            = 1.00e-4
ilut_fill       = 1.5
]])
AT_DATA([data-wall],[[# -*- getpot -*- (GetPot mode activation for emacs)
#-------------------------------------------------
#   Data file for the masstransport 
#-------------------------------------------------

[masstransport]

[./physics]
diffusivity     = 8.0e-9           # diffusivity
react           = -3.197e-4        # reaction coefficient
stationary      = 1                  # stationary calculation

# Till now we compute fluid and mastransport on the same mesh
[../discretization]
mesh_dir  = ../data/mesh/inria/         # the directory where the mesh file is
mesh_file = piece-of-tube-wall.mesh     # mesh file

[../aztec_o]
output          = warnings #all_res
max_iter        = 1550
poly_ord        = 5
kspace          = 40
precond         = dom_decomp
tol             = 1.00e-6
drop            = 1.00e-4
ilut_fill       = 1.5
]])
AT_CLEANUP
