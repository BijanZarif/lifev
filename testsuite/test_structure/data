[../miscellaneous]
factor    = 1
verbose   = 1


[../newton]
maxiter = 1
reltol  = 1.e-8


[../solver]
output          = none
max_iter        = 100
poly_ord        = 5
drop            = 1.00e-4
ilut_fill       = 4
tol             = 1.e-12

[../precond]

precond         = Amesos

reuse           = true
