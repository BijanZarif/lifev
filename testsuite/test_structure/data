# -*- getpot -*- (GetPot mode activation for emacs)
#-------------------------------------------------
#      Data file for Fluid Structure Solver
#-------------------------------------------------


[exporter]
multimesh  = false
start      = 0
save       = 1

[solid]

[./physics]
density   = 1.2               # density
young     = 3.0E6
poisson   = 0.3
endtime   = .02

[../discretization]
mesh_dir  = Mesh/     # the directory where the mesh file is
mesh_file = vessel2x4x20.mesh # mesh file
#mesh_file = hexa2x2.mesh # mesh file
timestep  = 0.001                   # timestep
order     = P1
mesh_faces      = all                   # update all faces elements
mesh_edges      = all                   # update all faces elements


[../miscellaneous]
factor    = 1
verbose   = 1


[../newton]
maxiter = 1
reltol  = 1.e-8


[../solver]
output          = none
max_iter        = 100
poly_ord        = 5
drop            = 1.00e-4
ilut_fill       = 4
tol             = 1.e-12

[../precond]

precond         = Amesos

reuse           = true
overlap         = 10
droptol         = 1.e-5
fill 		= 2

