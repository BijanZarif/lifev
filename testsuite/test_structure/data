[exporter]
multimesh  = false
start      = 0
save       = 1

[solid]

[./physics]
density   = 1.2               # density
young     = 3.0E6
poisson   = 0.3

[../time_discretization]
initialtime		= 0.
endtime			= 0.02
timestep		= 0.001

[../space_discretization]
mesh_dir  = Mesh/     # the directory where the mesh file is
