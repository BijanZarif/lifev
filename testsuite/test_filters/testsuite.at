AT_SETUP([test_filters])
AT_KEYWORDS([output import export])
AT_DATA([data],[[# -*- getpot -*- (GetPot mode activation for emacs)
#-------------------------------------------------
#      Data file for EthierSteinman test case
#-------------------------------------------------


[exporter]
multimesh  = false
start      = 0
save       = 1

[fluid]

[./physics]
density         = 1.0          # density
viscosity       = .01       # viscosity

[../time]
initialtime		= 0.
endtime			= 0.04
timestep		= 0.01
BDF_order		= 1

[../discretization]
mesh_dir        = ./Mesh/   # the directory where the mesh file is
mesh_file       = cube4x4.mesh          # mesh file
mesh_faces      = all                   # update all faces elements
verbose         = 0
linearized      = 0
diagonalize     = 1 # weight, 0=off
div_beta_u_v    = 0 # 1=on, 0=off
vel_order         = P1
press_order       = P1
mesh_faces      = all                   # update all faces elements
mesh_edges      = all                   # update all faces elements

[../miscellaneous]
verbose         = 1
steady          = 0
]])
AT_CHECK([ cp -p $(srcdir)/*.case  $(srcdir)/*.geo  $(srcdir)/*.scl $(srcdir)/*.vct  . &&
	   ln -sf ../../data/mesh/inria/cube4x4.mesh &&
           mpirun -np 4 ../../test_filters/test_filters -c],[0],[ignore],[ignore])
AT_CLEANUP([ethiersteinman.xmf ethiersteinman.h5])

